{
  "name": "koa-forge",
  "description": "Simple Starter Setup",
  "private": true,
  "version": "0.1.1",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build":
      "cross-env NODE_ENV=production run-s clean build:dist build:public",
    "build:dist": "rollup -c",
    "build:public": "webpack",
    "clean": "rimraf dist",
    "cosmos": "cosmos --config src/components/__cosmos__/config.js",
    "dev": "cross-env NODE_ENV=development node dev.js",
    "start": "node index.js",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "test:watch": "npm t -- --watch"
  },
  "esm": {
    "mode": "auto",
    "cjs": true,
    "await": true,
    "cache": true,
    "sourceMap": true,
    "debug": true
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "require": ["esm", "./test/register.js"]
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "koa": "^2.5.0",
    "koa-compress": "^2.0.0",
    "koa-helmet": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "koa-session": "^5.8.1",
    "koa-useragent": "^1.1.0",
    "microseconds": "^0.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/polyfill": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "ava": "^1.0.0-beta.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "brotli-webpack-plugin": "^0.5.0",
    "chokidar": "^2.0.3",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "esm": "^3.0.14",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "koa-webpack": "^3.0.2",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.21",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "postcss-modules": "^1.1.0",
    "postcss-scss": "^1.0.5",
    "prettier": "^1.11.1",
    "react-cosmos": "^4.0.0-beta.1",
    "react-cosmos-router-proxy": "^4.0.0-beta.1",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.4.1",
    "rollup-plugin-re": "^1.0.6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "supertest": "^3.0.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Francois-Esquire/koa-forge.git"
  },
  "homepage": "https://github.com/Francois-Esquire/koa-forge#readme",
  "bugs": {
    "url": "https://github.com/Francois-Esquire/koa-forge/issues"
  },
  "author": "Michael Tobia",
  "license": "MIT"
}
