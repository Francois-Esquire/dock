{
  "name": "koa-dock",
  "description": "Simple Starter Setup",
  "private": true,
  "version": "0.2.0",
  "main": "index.js",
  "engines": {
    "node": ">=7.6.0"
  },
  "env": {
    "host": "app.dev"
  },
  "deploy": {
    "image": "koa-forge",
    "registry": "michaeltobia",
    "tags": [
      "latest"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production node -r esm scripts/build.js",
    "deploy": "node -r esm scripts/deploy.js",
    "cosmos": "cosmos --config src/components/__cosmos__/config.js",
    "dev": "cross-env NODE_ENV=development node index.dev.js",
    "start": "node index.js",
    "ship": "run-s test build deploy",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "test-watch": "npm t -- --watch"
  },
  "esm": {
    "mode": "auto",
    "cjs": true,
    "await": true,
    "cache": true,
    "sourceMap": true,
    "debug": true
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "require": [
      "esm",
      "./test/register.js"
    ],
    "files": [
      "test/**/*.test.js"
    ],
    "sources": [
      "src/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "koa": "^2.5.1",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "koa-session": "^5.8.1",
    "koa-useragent": "^1.1.0",
    "microseconds": "^0.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "ava": "^1.0.0-beta.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.3",
    "brotli-webpack-plugin": "^1.0.0",
    "chokidar": "^2.0.3",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.3.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "esm": "^3.0.38",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "koa-webpack": "^4.0.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-modules": "^1.1.0",
    "postcss-scss": "^1.0.5",
    "prettier": "^1.12.1",
    "react-cosmos": "^4.4.0",
    "react-cosmos-router-proxy": "^4.4.0",
    "react-hot-loader": "^4.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.3",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-cleanup": "^3.0.0-beta.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-re": "^1.0.7",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "supertest": "^3.1.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Francois-Esquire/koa-dock.git"
  },
  "homepage": "https://github.com/Francois-Esquire/koa-dock#readme",
  "bugs": {
    "url": "https://github.com/Francois-Esquire/koa-dock/issues"
  },
  "author": "Michael Tobia",
  "license": "MIT"
}
