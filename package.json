{
  "name": "koa-forge",
  "description": "Simple Starter Setup",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production run-s clean build:public build:dist",
    "build:dist": "rollup -c",
    "build:public": "webpack",
    "build:watch": "run-p build:dist -- -w",
    "clean": "rimraf public && rimraf dist",
    "start": "node index.js",
    "test": "NODE_ENV=test ava --verbose",
    "test:watch": "npm t -- --watch"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "koa": "^2.4.1",
    "koa-compress": "^2.0.0",
    "koa-helmet": "^3.3.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.2",
    "koa-session": "^5.8.1",
    "koa-useragent": "^1.0.0",
    "microseconds": "^0.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/polyfill": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "@babel/register": "^7.0.0-beta.39",
    "ava": "0.25.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "chokidar": "^2.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "koa-webpack": "^2.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "1.10.2",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.1",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "supertest": "^3.0.0",
    "webpack": "^3.10.0"
  },
  "author": "Michael Tobia",
  "license": "MIT",
  "homepage": "https://github.com/Francois-Esquire/koa-forge#readme",
  "bugs": {
    "url": "https://github.com/Francois-Esquire/koa-forge/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Francois-Esquire/koa-forge.git"
  }
}