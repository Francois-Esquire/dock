{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "jsx": true
  },
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "babel",
    "react",
    "import",
    "jsx-a11y",
    "compat"
  ],
  "rules": {
    "indent": ["error", 2]
  }
}