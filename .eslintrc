{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "jsx": true
  },
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": ["babel", "react", "import", "jsx-a11y", "compat", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "indent": ["error", 2]
  }
}
