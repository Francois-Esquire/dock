{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "forceAllTransforms": false,
      "useBuiltIns": "usage",
      "spec": true,
      "modules": false,
      "targets": {
        "browsers": "last 2 versions"
      }
    }]
  ],
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-react", { "development": true }]
      ],
      "plugins": ["react-hot-loader/babel"]
    }
  }
}