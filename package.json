{
  "name": "koa-forge",
  "description": "Simple Starter Setup",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build":
      "cross-env NODE_ENV=production run-s clean build:public build:dist",
    "build:dist": "rollup -c",
    "build:public": "webpack",
    "clean": "rimraf public && rimraf dist",
    "dev": "cross-env NODE_ENV=development node index.js",
    "start": "node index.js",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "test:watch": "npm t -- --watch"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "koa": "^2.5.0",
    "koa-compress": "^2.0.0",
    "koa-helmet": "^3.3.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "koa-session": "^5.8.1",
    "koa-useragent": "^1.0.1",
    "microseconds": "^0.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/polyfill": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "@std/esm": "^0.21.7",
    "ava": "^1.0.0-beta.3",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "chokidar": "^2.0.2",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "ignore-styles": "^5.0.1",
    "koa-webpack": "^2.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "1.10.2",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.1",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-re": "^1.0.6",
    "supertest": "^3.0.0",
    "webpack": "^3.11.0"
  },
  "@std/esm": {
    "esm": "all",
    "cjs": true,
    "cache": true,
    "debug": true,
    "sourceMap": true
  },
  "ava": {
    "sources": ["src/*.{js,jsx}", "!dist/**/*", "!public/**/*"],
    "files": ["spec/*.test.{js,jsx}"],
    "require": ["@std/esm", "./spec/register.js"],
    "compileEnhancements": false,
    "babel": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Francois-Esquire/koa-forge.git"
  },
  "homepage": "https://github.com/Francois-Esquire/koa-forge#readme",
  "bugs": {
    "url": "https://github.com/Francois-Esquire/koa-forge/issues"
  },
  "author": "Michael Tobia",
  "license": "MIT"
}
